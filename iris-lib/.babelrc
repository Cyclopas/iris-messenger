{
  "plugins": [
    ["transform-flow-strip-types"], ["transform-runtime"]
  ],
  "env": {
    "test": {
      "presets": [
        ["es2015", {"loose": true}]
      ],
      "plugins": [
        ["add-module-exports"]
      ]
    },
    "cjs": {
      "presets": [
        ["es2015", {"loose": true}]
      ],
      "plugins": [
        ["add-module-exports"], ["transform-runtime"]
      ]
    },
    "es": {
      "presets": [
        ["es2015", {"loose": true, "modules": false}]
      ]
    }
  }
}
